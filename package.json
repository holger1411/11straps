{
  "name": "11strap",
  "version": "0.0.1",
  "description": "simple 11ty and Bootstrap5 boilerplate",
  "license": "MIT",
  "private": "true",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "main": "index.html",
  "scripts": {
    "build": "npm run clean && npm run clean-prod  && npx eleventy && gulp sass && gulp minify-css && gulp dist-assets && gulp prod-copy && gulp inject-min-css && gulp purgecss && gulp minify-html",
    "build-dev": "eleventy && gulp dist-assets && gulp sass",
    "11ty": "eleventy && gulp inject-css",
    "sync": "gulp browser-sync",
    "watch-dev": "npm run build-dev && npm run sync",
    "postinstall": "gulp copy-assets",
    "scss": "node-sass --output-style compressed -o dev/css/ src/scss/theme.scss",
    "watch:all": "onchange 'src/' -- npm run build-dev",
    "serve": "eleventy --serve",
    "watch": "npm run clean && npm run build-dev && run-p sync watch:all",
    "clean": "rm -rf dev/*",
    "clean-prod": "rm -rf public/*"
  },
  "dependencies": {
    "@11ty/eleventy": "0.11.1",
    "bootstrap": "5.0.0-beta1",
    "bootstrap-icons": "1.2.2",
    "clean-css": "^4.2.3",
    "gulp": "^4.0.2",
    "gulp-clean": "^0.4.0",
    "gulp-clean-css": "^4.2.0",
    "gulp-htmlmin": "^5.0.1",
    "gulp-imagemin": "^7.1.0",
    "gulp-plumber": "^1.2.1",
    "gulp-rename": "^2.0.0",
    "gulp-sass": "4.1.0",
    "npm-watch": "^0.7.0",
    "sass": "^1.32.2",
    "watch": "^1.0.2"
  },
  "author": "Holger Koenemann",
  "devDependencies": {
    "gulp-html-replace": "^1.6.2",
    "gulp-purgecss": "^3.1.3",
    "node-sass": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "onchange": "^7.0.2",
    "react-is": "^17.0.1"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "keywords": [],
  "bugs": {
    "url": ""
  },
  "homepage": ""
}
